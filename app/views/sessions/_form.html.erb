<%= form_with url: sessions_path, local: true do |form| %>

  <%= render 'shared/flash_message' %>

  <p>
    <%= form.label :email %><br>
    <%= form.text_field :email, id: :email %>
  </p>

  <p>
    <%= form.label :password %><br>
    <%= form.text_field :password, id: :password %>
  </p>

  <p>
    <%= form.submit 'Enter ' %>
  </p>

<% end %>