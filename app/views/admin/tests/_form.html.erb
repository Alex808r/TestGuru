<%= form_with model: [:admin, @test], local: true do |form| %>

  <%= render "shared/errors", object: @test %>

  <p>
    <%= form.label :title %><br>
    <%= form.text_field :title, id: :test_title%>
  </p>

  <p>
    <%= form.label :level %><br>
    <%= form.number_field :level, id: :test_level %>
  </p>

  <p>
    <%= form.label :category_id %><br>
    <%= form.collection_select :category_id, Category.all, :id, :title, prompt: true %>
  </p>

  <p>
    <%= form.label :passing_time, t('.passing_time') %><br>
    <%= form.number_field :passing_time, id: :test_passing_time %>
  </p>

  <p> <%= form.submit %> </p>

<% end %>
