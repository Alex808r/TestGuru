<%= form_with model: [:admin, @badge], local: true do |form| %>

  <%= render "shared/errors", object: @badge %>
  <p>
    <%= form.label :title %><br>
    <%= form.text_field :title, id: :badge_title %>
  </p>

  <p>
    <%= form.label :image_url %><br>
    <%= form.text_field :image_url, id: :badge_image_url %>
  </p>

  <p>
    <%= form.label :rule %><br>
    <%= form.text_field :rule, id: :badge_rule %>
  </p>

  <p>
    <%= form.label :parameter %><br>
    <%= form.text_field :parameter, id: :badge_parameter %>
  </p>

  <p> <%= form.submit %> </p>
  <p> <%#= form.submit(@test.new_record? ? "Create" : "Update") %> </p>
<% end %>